{
  "name": "TypiCMS",
  "dependencies": {
    "jquery": "~2.*",
    "alertify.js": "~0.3.*",
    "bootstrap": "~3.1.*",
    "dropzone": "~3.*",
    "jquery-ui": "~1.10.*",
    "fancybox": "~2.1.*",
    "flexslider": "*",
    "font-awesome": "~4.*",
    "eonasdan-bootstrap-datetimepicker": "~3.*",
    "selectize": "~0.*",
    "pickadate": "~3.5.2"
  },
  "overrides": {
    "fancybox": {
      "main": [
        "source/jquery.fancybox.js"
      ]
    },
    "bootstrap": {
      "main": [
        "js/dropdown.js",
        "js/collapse.js",
        "js/alert.js",
        "js/tab.js",
        "js/transition.js"
      ]
    },
    "jquery-ui": {
      "main": [
        "ui/jquery-ui.js",
        "ui/jquery.ui.core.js",
        "ui/jquery.ui.mouse.js",
        "ui/jquery.ui.widget.js",
        "ui/jquery.ui.sortable.js"
      ]
    },
    "flexslider": {
      "main": [
        "jquery.flexslider.js"
      ]
    },
    "font-awesome": {
      "ignore": true
    }
  }
}
