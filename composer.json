{
	"name": "sdebacker/typicms",
	"description": "Simple multilingual CMS",
	"version": "1.0.0-dev",
	"keywords": ["CMS", "multilingual", "laravel", "Typi"],
	"license": "MIT",
	"authors": [
		{
			"name": "Typi Design",
			"email": "info@typidesign.be"
		},
		{
			"name": "Samuel De Backer",
			"email": "info@typidesign.be",
			"role": "Developer"
		}
	],
	"repositories": [{
		"type": "vcs",
		"url": "https://github.com/sdebacker/croppa"
	}],
	"require": {
		"bkwld/croppa": "dev-master",
		"cartalyst/sentry": "~2.1",
		"davejamesmiller/laravel-breadcrumbs": "~2.2.1",
		"dimsav/laravel-translatable": "~4.0",
		"edvinaskrucas/notification": "~2.0",
		"eluceo/ical": "~0.1",
		"jwage/easy-csv": "~0.0.2",
		"laravel/framework": "~4.2.1",
		"mccool/database-backup": "~1.0.0",
		"msurguy/honeypot": "~0.2",
		"thomaswelton/laravel-gravatar": "~0.0.2"
	},
	"require-dev": {
		"barryvdh/laravel-debugbar": "1.*",
		"barryvdh/laravel-ide-helper": "~1.9",
		"leroy-merlin-br/larasniffer": "~1.0.2",
		"mockery/mockery": "0.9.*@dev",
		"phpspec/phpspec": "2.0.*@dev",
		"raveren/kint": "dev-master"
	},
	"autoload": {
		"classmap": [
			"app/commands",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/TestCase.php"
		],
		"psr-4": {
			"TypiCMS\\": "app/TypiCMS"
		}
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan optimize"
		],
		"pre-update-cmd": [
			"php artisan clear-compiled"
		],
		"post-update-cmd": [
			"php artisan optimize",
			"php artisan ide-helper:generate"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
