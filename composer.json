{
	"name": "sdebacker/typicms",
	"description": "Simple multilingual CMS",
	"version": "1.0.0-dev",
	"keywords": ["CMS", "multilingual", "laravel", "Typi"],
	"license": "MIT",
	"authors": [
		{
			"name": "Typi Design",
			"email": "info@typidesign.be"
		},
		{
			"name": "Samuel De Backer",
			"email": "info@typidesign.be",
			"role": "Developer"
		}
	],
	"require": {
		"bkwld/croppa": "~3.2.1",
		"cartalyst/sentry": "~2.1",
		"davejamesmiller/laravel-breadcrumbs": "~2.2.1",
		"dimsav/laravel-translatable": "~4.0",
		"edvinaskrucas/notification": "~2.0",
		"eluceo/ical": "~0.1",
		"guzzlehttp/guzzle": "~4.0",
		"jwage/easy-csv": "~0.0.2",
		"laravel/framework": "~4.2.1",
		"mccool/database-backup": "~1.0.1",
		"msurguy/honeypot": "~0.2",
		"thomaswelton/laravel-gravatar": "~0.0.2",
		"cviebrock/image-validator": "~1.0.3"
	},
	"require-dev": {
		"way/generators": "~2.6",
		"barryvdh/laravel-debugbar": "~1.6",
		"leroy-merlin-br/larasniffer": "~1.0.2",
		"mockery/mockery": "~0.9",
		"phpspec/phpspec": "~2.0",
		"raveren/kint": "~0.9"
	},
	"autoload": {
		"classmap": [
			"app/commands",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/TestCase.php"
		],
		"psr-4": {
			"TypiCMS\\": "app/TypiCMS"
		}
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "stable"
}
